					<stddev>0.005</stddev>
				</noise>
			</ray>
			<plugin name="gazebo_ros_lds_lfcd_controller_rear" filename="libgazebo_ros_laser.so">
				<topicName>rear_scan</topicName>
				<frameName>rear_scan_link</frameName>
			</plugin>
		</sensor>
	</gazebo>


