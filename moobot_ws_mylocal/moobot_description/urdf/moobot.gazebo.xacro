	<gazebo>
		<plugin name="agv_controller" filename="libgazebo_ros_diff_drive.so">
			<commandTopic>/cmd_vel</commandTopic>
			<odometryTopic>odom</odometryTopic>
			<odometryFrame>odom</odometryFrame>
			<odometrySource>world</odometrySource>
			<publishOdomTF>false</publishOdomTF>
			<robotBaseFrame>base_footprint</robotBaseFrame>
			<publishWheelTF>false</publishWheelTF>
			<publishTf>true</publishTf>
			<publishWheelJointState>false</publishWheelJointState>
			<legacyMode>false</legacyMode>
			<updateRate>30</updateRate>
			<leftJoint>RLM_wheel_joint</leftJoint>
			<rightJoint>RRM_wheel_joint</rightJoint>
			<wheelSeparation>0.4775</wheelSeparation>
			<wheelDiameter>0.21</wheelDiameter>
			<wheelAcceleration>1</wheelAcceleration>
			<wheelTorque>100</wheelTorque>
			<rosDebugLevel>na</rosDebugLevel>
		</plugin>
	</gazebo>

	<!-- Scanner Plugins -->

	<!-- Front Scanner -->
	<gazebo reference="front_scan_link">
		<material>Gazebo/YellowGlow</material>
		<sensor type="ray" name="lds_lfcd_sensor">
			<pose>0 0 0 0 0 0</pose>
			<visualize>$(arg laser_visual)</visualize>
			<update_rate>30</update_rate>
			<ray>
				<scan>
					<horizontal>
						<samples>1280</samples>
						<resolution>0.17</resolution>
						<min_angle>-2.34</min_angle>
						<max_angle>2.34</max_angle>
					</horizontal>
				</scan>
				<range>
					<min>0.010</min>
					<max>30</max>
					<resolution>0.01</resolution>
				</range>
				<noise>
					<type>gaussian</type>
					<mean>0.0</mean>
					<stddev>0.005</stddev>
				</noise>
			</ray>
			<plugin name="gazebo_ros_lds_lfcd_controller" filename="libgazebo_ros_laser.so">
				<topicName>front_scan</topicName>
				<frameName>front_scan_link</frameName>
			</plugin>
		</sensor>
	</gazebo>

	<!-- Rear Scanner -->

	<gazebo reference="rear_scan_link">
		<material>Gazebo/YellowGlow</material>
		<sensor type="ray" name="lds_lfcd_sensor_rear">
			<pose>0 0 0 0 0 0</pose>
			<visualize>$(arg laser_visual)</visualize>
			<update_rate>30</update_rate>
			<ray>
				<scan>
					<horizontal>
						<samples>1280</samples>
						<resolution>0.17</resolution>
						<min_angle>-2.34</min_angle>
						<max_angle>2.34</max_angle>
					</horizontal>
				</scan>
				<range>
					<min>0.010</min>
					<max>30</max>
					<resolution>0.01</resolution>
				</range>
				<noise>
